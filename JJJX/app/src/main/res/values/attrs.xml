<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--圆形头像-->
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="rce_tv_list_item">
        <attr name="tv_list_item_allow_arrow" format="boolean" />
        <attr name="tv_list_item_title" format="string|reference" />
        <attr name="tv_list_item_title_color" format="color" />
        <attr name="tv_list_item_detail" format="string" />
        <attr name="tv_list_item_detail_color" format="color" />
        <attr name="tv_list_item_title_icon" format="reference" />
    </declare-styleable>

</resources>